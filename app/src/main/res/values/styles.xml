<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--<item name="android:windowBackground">#f8f8f8</item>-->
        <item name="android:configChanges">orientation|keyboardHidden</item>
        <item name="android:screenOrientation">portrait</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden|stateHidden|adjustPan</item>
    </style>

    <style name="HalfTransparentAppTheme" parent="AppTheme">
        <item name="android:windowBackground">@color/bantouming</item>
        　　
        <!--<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>-->
    </style>

    <style name="TransparentAppTheme" parent="AppTheme">
        <item name="android:windowBackground">@color/touming</item>
        　　
        <!--<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>-->
    </style>


    <style name="main_tabcontent_item_style" parent="font_small3">
        <!-- Customize your theme here. -->
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">@dimen/dp_6</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">@dimen/dp_10</item>
        <item name="android:paddingTop">@dimen/dp_10</item>
        <item name="android:textColor">@color/black_gray</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:height">@dimen/dp_0.5</item>
        <item name="android:background">@color/black_gray</item>
    </style>

    <style name="index_menu_button_style" parent="font_black_small2">
